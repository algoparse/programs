
class C {
    private int a
    public int b
    protected int c

    constructor(int a, int b) {}

    set a {
        this.a = value
    }
    get a {
        return this.a
    }

    set c {} // does default setting behind the scenes
    get c {} // does default getting behind the scenes

    int sum() {
        return a + b
    }
}

class C2 extends C {
    int sum() {
        return base.a + c
    }
}

int main() {
    C2 c = C2(1, 2)
    c.c = 3
    println(c.sum())
}